# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Linkup API Configuration (for web search)
LINKUP_API_KEY=your_linkup_api_key_here
LINKUP_BASE_URL=https://api.linkup.so/v1

# Airia API Configuration (for agent orchestration)
AIRIA_API_KEY=your_airia_api_key_here
AIRIA_BASE_URL=https://api.airia.ai/v2

# Airia Agent Pipeline IDs (GUIDs)
AIRIA_SALES_AGENT_ID=your_sales_agent_id_here
AIRIA_CS_AGENT_ID=your_cs_agent_id_here
AIRIA_RESEARCH_AGENT_ID=your_research_agent_id_here

# Enable Airia orchestration (true/false)
USE_AIRIA_ORCHESTRATION=false

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MINUTES=15
RATE_LIMIT_MAX_REQUESTS=100
AI_RATE_LIMIT_MAX_REQUESTS=10
MAX_TOKENS_PER_REQUEST=2000

# Daily Budget Limit (in USD) - set to 0 to disable
DAILY_BUDGET_LIMIT=5
