# Copy this file to `.env` and fill in your real keys. Do NOT commit `.env` to git.

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-key-here

# Linkup API Configuration (for web search)
LINKUP_API_KEY=your-linkup-key-here
LINKUP_BASE_URL=https://api.linkup.so/v1

# Airia API Configuration (for agent orchestration)
AIRIA_API_KEY=your-airia-key-here
AIRIA_BASE_URL=https://api.airia.ai/v2

# Airia Agent Pipeline IDs (GUIDs)
AIRIA_SALES_AGENT_ID=0fd0347f-27c2-4205-85ba-0d65934172b1
AIRIA_CS_AGENT_ID=7401dfba-ed4b-470c-bbb5-91bfe4e4be42
AIRIA_RESEARCH_AGENT_ID=0e4fde0c-1d36-47c6-bab5-e3c4e52ccae2

# Enable Airia orchestration (true/false)
USE_AIRIA_ORCHESTRATION=false
